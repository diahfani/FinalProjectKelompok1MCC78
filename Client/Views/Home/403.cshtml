@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="en" >
<head>
  <meta charset="UTF-8">
  <title>403 Forbidden</title>
  <style>
* {
  position: relative;
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  font-family: "Lato", sans-serif;
}

body {
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: white;
}

h1 {
  margin: 40px 0 20px;
  color: blue;
}

.lock {
  border-radius: 5px;
  width: 55px;
  height: 45px;
  background-color: #333;
  animation: dip 1s;
  animation-delay: 1.5s;
}
.lock::before, .lock::after {
  content: "";
  position: absolute;
  height: 20px;
  width: 15px;
  left: calc(50% - 12.5px);
}
.lock::before {
  top: -30px;
  border: 5px solid #333;
  border-bottom-color: transparent;
  border-radius: 15px 15px 0 0;
  height: 30px;
  animation: lock 2s, spin 2s;
}
.message a {
                font-family: 'Montserrat', sans-serif;
                font-size: 14px;
                text-decoration: none;
                text-transform: uppercase;
                background: #105eff;
                display: inline-block;
                padding: 15px 30px;
                border-radius: 40px;
                color: #fff;
                font-weight: 700;
                margin-top: 30px;
                margin-left: 130px;
            }
  </style>

</head>
<body>
<!-- partial:index.partial.html -->
<div class="lock"></div>
<div class="message">
  <h1>Access to this page is forbidden</h1>
  <p>Please check with the site admin if you believe this is a mistake.</p>
  <a asp-controller="Home" asp-action="LandingPage">Go TO LandingPage</a>
</div>
<!-- partial -->
  
</body>
</html>
